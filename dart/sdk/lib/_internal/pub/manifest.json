{
  "manifest_version": 2,
  "name": "Tavern (dart)",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {"128": "images/dart_icon.png"},
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage",
    "https://pub.dartlang.org/",
    "https://commondatastorage.googleapis.com/pub.dartlang.org",
    "https://tavern.appspot.com/",
    "http://pajamallama0.syd.corp.google.com:8080/"
  ]
}
