{
  "manifest_version": 2,
  "name": "Tavern (dart)",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {"128": "images/dart_icon.png"},
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ]
}
